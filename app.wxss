/**app.wxss**/
@import 'resource/iconfont/iconfont';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
  font-size: 30rpx;
} 
.text-title {
  padding: 15rpx 30rpx;
  color: #555;
}
.item{
  padding: 30rpx;
  border-bottom: 3rpx solid #aaa;
}
.footer{
  position: absolute;
  align-self:center;
  flex:1;
  display:flex;
  align-items:flex-end;
  bottom: 20rpx;
  font-size: 28rpx;
  color:#999;
}